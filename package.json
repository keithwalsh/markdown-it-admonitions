{
  "name": "markdown-it-admonitions",
  "version": "0.1.6",
  "description": "A markdown-it plugin for rendering admonitions",
  "type": "module",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "admonitions",
    "admonition",
    "callouts",
    "markdown"
  ],
  "homepage": "https://github.com/keithwalsh/markdown-it-admonitions#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/keithwalsh/markdown-it-admonitions.git"
  },
  "bugs": {
    "url": "https://github.com/keithwalsh/markdown-it-admonitions/issues"
  },
  "license": "MIT",
  "author": "Keith Walsh",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./admonitions.css": "./admonitions.css",
    "./admonitions.min.css": "./admonitions.min.css"
  },
  "files": [
    "dist",
    "admonitions.css",
    "admonitions.min.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run build:css",
    "build:css": "csso admonitions.css -o admonitions.min.css",
    "preview": "vite preview",
    "test:visual": "serve . -p 3000"
  },
  "peerDependencies": {
    "markdown-it": "^13.0.0 || ^14.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "csso-cli": "^4.0.2",
    "markdown-it": "^14.1.0",
    "react-icons": "^5.5.0",
    "serve": "^14.2.5",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0"
  }
}
